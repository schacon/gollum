<div id="wiki-wrapper" class="page">
<div id="head">
  <h1>{{title}}</h1>
  <ul class="actions">
    <li class="minibutton"><a href="/">Home</a></li>
    <li class="minibutton"><a href="/document/pdf">PDF</a></li>
    <li class="minibutton"><a href="/document/mobi">Mobi</a></li>
    <li class="minibutton"><a href="/document/epub">ePub</a></li>
  </ul>
  {{>searchbar}}
</div>
<div id="wiki-content">
<div class="wrap {{#has_sidebar}}has-rightbar{{/has_sidebar}}">
  <div id="wiki-body" class="gollum-{{format}}-content">
    <div id="template">
      <div id="content">Loading document...</div>
    </div>
  </div>
  <div id="wiki-rightbar" class="gollum-{{sidebar_format}}-content">
    loading table of contents
  </div>
</div>
</div>
</div>

<script type="text/javascript" src="/javascript/jquery.js"></script>
<script type="text/javascript">
  $(function() {
    $('#content').load("/document/html")
  });
</script>

<script type="text/javascript" src="/javascript/MathJax/MathJax.js"></script>
